    "strict": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "components/avatar/**/*",
    "node_modules",
    "**/node_modules/**"
  ]
}
