{
  "expo": {
    "sdkVersion": "54.0.0",
    "scheme": "yachai",
    "name": "Yachai",
    "slug": "yachai",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icona.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icona.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "teamId": "V4VB38G95T",
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true,
        "aps-environment": "development"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.labellapartners.wellnesscoach",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to analyze your skin and detect emotions for personalized wellness coaching.",
        "NSMicrophoneUsageDescription": "Allow Yachai to access your microphone for voice interactions.",
        "NSSpeechRecognitionUsageDescription": "Allow Yachai to use speech recognition for voice commands.",
        "NSHealthShareUsageDescription": "This app accesses health data to provide personalized wellness recommendations.",
        "NSHealthUpdateUsageDescription": "This app writes wellness data to Apple Health for comprehensive health tracking.",
        "NSCalendarsUsageDescription": "This app needs access to your calendar to sync wellness activities and reminders.",
        "NSRemindersUsageDescription": "This app needs access to your reminders to set wellness activity notifications."
      },
      "bitcode": false,
      "appleTeamId": "V4VB38G95T"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icona.png",
        "backgroundColor": "#6366f1"
      },
      "notification": {
        "icon": "./assets/icona.png",
        "color": "#6366f1"
      },
      "package": "com.wellnesscoach.app",
      "statusBar": {
        "backgroundColor": "#00000000",
        "translucent": true,
        "barStyle": "light-content"
      },
      "navigationBar": {
        "backgroundColor": "#0b0b14",
        "barStyle": "light-content",
        "visibility": "leanback"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.BODY_SENSORS",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_MICROPHONE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.INTERNET",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_DISTANCE",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_RESTING_HEART_RATE",
        "android.permission.health.READ_HEART_RATE_VARIABILITY",
        "android.permission.health.READ_SLEEP",
        "android.permission.health.READ_TOTAL_CALORIES_BURNED",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.READ_BLOOD_PRESSURE",
        "android.permission.health.READ_WEIGHT",
        "android.permission.health.READ_BODY_FAT",
        "android.permission.health.READ_MENSTRUATION",
        "android.permission.health.READ_HYDRATION",
        "android.permission.health.READ_NUTRITION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icona.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-audio-studio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for voice chat"
        }
      ],
      "@livekit/react-native-expo-plugin",
      "@config-plugins/react-native-webrtc",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Yachai to access your camera for skin analysis and emotion detection."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Yachai to save analysis photos to your photo library.",
          "savePhotosPermission": "Allow Yachai to save analysis results.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow Yachai to sync wellness activities with your calendar."
        }
      ],
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Allow Yachai to access your microphone for voice interactions.",
          "speechRecognitionPermission": "Allow Yachai to use speech recognition for voice commands."
        }
      ],
      [
        "react-native-health",
        {
          "NSHealthShareUsageDescription": "This app accesses health data to provide personalized wellness recommendations.",
          "NSHealthUpdateUsageDescription": "This app writes wellness data to Apple Health for comprehensive health tracking."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icona.png",
          "color": "#6366f1",
          "defaultChannel": "default"
        }
      ],
      "react-native-health-connect",
      "./plugins/with-health-connect-permission-delegate",
      "expo-asset",
      "expo-audio",
      "expo-sqlite",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "156857d0-adfa-4211-8446-1e79c690446b"
      }
    },
    "owner": "lbp_manager"
  }
}
